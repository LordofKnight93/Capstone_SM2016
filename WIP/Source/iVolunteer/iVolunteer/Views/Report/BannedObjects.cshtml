@model Tuple<List<iVolunteer.Models.MongoDB.EmbeddedClass.LinkClass.SDLink>,List<iVolunteer.Models.MongoDB.EmbeddedClass.LinkClass.SDLink>,List<iVolunteer.Models.MongoDB.EmbeddedClass.LinkClass.SDLink>>

<div class="card-panel" style="margin: 0.5rem 0 1rem 0;">
    <div class="row" style="margin-top: -18px; margin-bottom: -5px;">
        <div class="col-sm-12">
            <h4>Danh sách nhóm bị đình chỉ</h4>
        </div>
    </div>
    <table class="table">
        @foreach (var group in Model.Item1)
        {
            <tr>
                <td><a>@group.DisplayName</a></td>
                <td>
                    <input type="button" class="btn btn-success" value="Kích hoạt" onclick="location.href ='@Url.Action("ReactivateGroup","Report", new { groupID = group.ID })'" />
                </td>
            </tr>
        }
    </table>
</div>
<div class="card-panel" style="margin: 0.5rem 0 1rem 0;">
    <div class="row" style="margin-top: -18px; margin-bottom: -5px;">
        <div class="col-sm-12">
            <h4>Danh sách dự án bị đình chỉ</h4>
        </div>
    </div>
    <table class="table">
        @foreach (var project in Model.Item2)
        {
            <tr>
                <td><a>@project.DisplayName</a></td>
                <td>
                    <input type="button" class="btn btn-success" value="Kích hoạt" onclick="location.href ='@Url.Action("ReactivateProject","Report", new { projectID = project.ID })'" />
                </td>
            </tr>
        }
    </table>
</div>
<div class="card-panel" style="margin: 0.5rem 0 1rem 0;">
    <div class="row" style="margin-top: -18px; margin-bottom: -5px;">
        <div class="col-sm-12">
            <h4>Danh sách người dùng bị đình chỉ</h4>
        </div>
    </div>
    <table class="table">
        @foreach (var user in Model.Item3)
        {
            <tr>
                <td><a>@user.DisplayName</a></td>
                <td>
                    <input type="button" class="btn btn-success" value="Kích hoạt" onclick="location.href ='@Url.Action("ReactivateUser","Report", new { userID = user.ID })'" />
                </td>
            </tr>
        }
    </table>
</div>
