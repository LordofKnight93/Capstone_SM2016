@Html.Partial("_PostCreate")
<div id="postList">
    @{Html.RenderAction("GetPostList", "Group", new { groupID = ViewBag.GroupID });}
</div>
<div class="panel-footer" style="text-align: center">
    <a role="discussion" id="@("loadMore" + ViewBag.GroupID)" href="" onclick="@("loadMorePost(event,'" + ViewBag.GroupID +"', this.getAttribute('role'))")">Load more</a>
</div>