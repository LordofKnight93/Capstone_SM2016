@model IEnumerable<iVolunteer.Models.SQL.DBContext.Account>

<p>
    @Html.ActionLink("Register", "Register","Account")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Password)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FacebookLink)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IndentifyID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsAdmin)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActivate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsConfirm)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Password)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FacebookLink)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IndentifyID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsAdmin)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActivate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsConfirm)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit","Account", new { id = item.UserID },null) |
            @Html.ActionLink("Details", "Details", "Account", new { id = item.UserID },null) |
            @Html.ActionLink("Delete", "Delete", "Account",new { id = item.UserID },null)
        </td>
    </tr>
}

</table>
